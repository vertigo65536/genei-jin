import aiohttp, urllib, os, re, brickfront
from tools import updateCounter, searchResultsTest
from dotenv import load_dotenv

load_dotenv()
client = brickfront.Client(str(os.getenv('BRICKSET_API')))

# Searches youtube, taking a query and a result number as parameters
# and returning a url

async def search(query, n, prefix=None): 
    setList = client.getSets(query=query)
    build = {}
    build['name'] = setList[n].name
    build['variant'] = setList[n].variant
    build['year'] = setList[n].year
    build['image'] = setList[n].imageURL
    build['url'] = setList[n].bricksetURL
    build['released'] = setList[n].released
    build['price'] = setList[n].priceUK
    build['description'] = setList[n].description
    build['rating'] = setList[n].rating
    return build

